#WC up and down
record(waveform, "TR72NW_socket1:DATA")
{
	field(DESC, "TR72NW_socket raw data")
	field(FTVL, "FLOAT")
	field(NELM, "2")
	field(SCAN, "10 second")
	field(PHAS, "0")
	field(DTYP, "TR72NW_socketSup")
#	field(DESC, "HTTP@192.168.30.35&SN@0xB2,0x6B,0x18,0x52")
	field(DESC, "HTTP@192.168.30.35")
#	field(DESC, "SN@0xB2,0x6B,0x18,0x52")
	field(FLNK, "AIR:WCup:TEMP")
}
record(subArray, "AIR:WCup:TEMP")
{
	field(SCAN, "Passive")
	field(INP,  "TR72NW_socket1:DATA")
	field(FTVL, "FLOAT")
	field(MALM, "2")
	field(NELM, "1")
	field(PREC, "1")
	field(INDX, "0")
	field(FLNK, "AIR:WCup:HUMI")
}
record(subArray, "AIR:WCup:HUMI")
{
	field(SCAN, "Passive")
	field(INP,  "TR72NW_socket1:DATA")
	field(FTVL, "FLOAT")
	field(MALM, "2")
	field(NELM, "1")
	field(PREC, "1")
	field(INDX, "1")
}

