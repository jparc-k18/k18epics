#BFT tent
record(waveform, "TR700W1:DATA")
{
	field(DESC, "TR700W raw data")
	field(FTVL, "FLOAT")
	field(NELM, "2")
	field(SCAN, "10 second")
	field(PHAS, "0")
	field(DTYP, "TR700WSup")
	field(DESC, "HTTP@192.168.30.31")
	field(FLNK, "AIR:BFT_TENT:TEMP")
}
record(subArray, "AIR:BFT_TENT:TEMP")
{
	field(SCAN, "Passive")
	field(INP,  "TR700W1:DATA")
	field(FTVL, "FLOAT")
	field(MALM, "2")
	field(NELM, "1")
	field(PREC, "1")
	field(INDX, "0")
	field(FLNK, "AIR:BFT_TENT:HUMI")
}
record(subArray, "AIR:BFT_TENT:HUMI")
{
	field(SCAN, "Passive")
	field(INP,  "TR700W1:DATA")
	field(FTVL, "FLOAT")
	field(MALM, "2")
	field(NELM, "1")
	field(PREC, "1")
	field(INDX, "1")
}
