record(waveform, "PMX:SDC$(id_num):DATA")
{
	field(DESC, "IP:$(ip)")
	field(FTVL, "FLOAT")
	field(NELM, "2")
	field(SCAN, "10 second")
	field(PHAS, "0")
	field(DTYP, "PMX18Sup")
	field(FLNK, "PMX:SDC$(id_num):VTH")
}
record(subArray, "PMX:SDC$(id_num):VTH")
{
	field(SCAN, "Passive")
	field(INP,  "PMX:SDC$(id_num):DATA")
	field(FTVL, "FLOAT")
	field(MALM, "2")
	field(NELM, "1")
	field(PREC, "1")
	field(INDX, "0")
	field(FLNK, "PMX:SDC$(id_num):CUR")
}
record(subArray, "PMX:SDC$(id_num):CUR")
{
	field(SCAN, "Passive")
	field(INP,  "PMX:SDC$(id_num):DATA")
	field(FTVL, "FLOAT")
	field(MALM, "2")
	field(NELM, "1")
	field(PREC, "1")
	field(INDX, "1")
}
