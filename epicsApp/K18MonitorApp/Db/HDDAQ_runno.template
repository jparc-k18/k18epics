record(waveform, "HDDAQ:RUNNUMBER:DATA")
{
        field(DESC, "HDDAQ Run Number")
	field(FTVL, "FLOAT")
        field(NELM, "100")
	field(SCAN, "5 second")
	field(PHAS, "0")
	field(DTYP, "HDDAQ_Sup")
}

record(ai, "$(name)")
{
	field(SCAN, "10 second")
	field(INP,  "HDDAQ:RUNNUMBER:DATA")
	field(DTYP, "HDDAQ_Sup")
	field(PHAS, "1")
	field(PREC, "4")
	field(DESC, "HDDAQ@$(ch)")
}
